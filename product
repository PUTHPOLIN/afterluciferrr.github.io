    <script>
    $(document).ready(function(){
        $('.do-order').click(function(){
            var p_id = $(this).val();
            $('#message').text( 'Are you sure want to purchase '+ $("#product-name-"+p_id).text());
            $('#app_id').val(p_id);
            $('#modal-default').modal('show');
       
        });
    });
